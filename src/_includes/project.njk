---
layout: base.njk
---

<article>
  <h2 class="inline">{{ title }}</h2>
    <span class="dates">
      (<time datetime="{{ date }}">{{ date | date('year') }}</time>{% if endDate %} - <time datetime="{{ endDate }}">{{ endDate | date('year') }}</time>{% endif %})
    </span>
  {{ content | safe }}
</article>
