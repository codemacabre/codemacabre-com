---
layout: base.njk
---

<article>
  <h2>{{ title }}</h2>
  <p>
    <span class="category">{{ status.toUpperCase() }}</span>
    <span class="dates">
      <time datetime="{{ date }}">{{ date | date('year') }}</time>{% if endDate %} - <time datetime="{{ endDate }}">{{ endDate | date('year') }}</time>{% endif %}
    </span>
  </p>
  {{ content | safe }}
</article>
