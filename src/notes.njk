---
layout: base.njk
title: Notes
description: Notes by Myles Lewando
pagination:
  data: collections.notes
  size: 10
  reverse: true
---

<div class="heading-and">
  <h2>{{ title }}</h2>
  <a class="rss right" href="/feed.xml">
    RSS
  </a>
</div>
<ul class="wider">
{% for post in pagination.items %}
  <li>
    <a class="title" href="{{ post.url }}">{{ post.data.title }}</a> <span class="meta"><span class="category">{{ post.data.category }}</span> <span class="dates">(<time datetime="{{ post.data.date }}">{{ post.data.date | date('full') }}</time>)</span></span>
    <p class="description">{{ post.data.description }}</p>
  </li>
{% endfor %}
</ul>

{% include 'pagination.njk' %}
